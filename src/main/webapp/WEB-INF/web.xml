    <display-name>AjaxNavbarServlet</display-name>
    <servlet-name>AjaxNavbarServlet</servlet-name>
    <servlet-class>com.revature.servlets.AjaxNavbarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxNavbarServlet</servlet-name>
    <url-pattern>/AjaxNavbarServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AjaxDashboardServlet</display-name>
