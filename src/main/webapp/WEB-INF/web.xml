  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxTxServlet</servlet-name>
    <url-pattern>/AjaxTxServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AjaxProcessTxServlet</display-name>
